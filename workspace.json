{
  "version": 2,
  "projects": {
    "keadex-docs": {
      "projectType": "application",
      "root": "apps/keadex-docs",
      "sourceRoot": "apps/keadex-docs/src",
      "targets": {
        "build": {
          "executor": "@nx-plus/docusaurus:browser",
          "options": {
            "outputPath": "dist/apps/keadex-docs"
          }
        },
        "serve": {
          "executor": "@nx-plus/docusaurus:dev-server",
          "options": {
            "port": 3001
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/keadex-docs/**/*.{ts,tsx,js,jsx}"],
            "eslintConfig": "apps/keadex-docs/.eslintrc.json",
            "ignorePath": "apps/keadex-docs/.eslintignore"
          }
        }
      },
      "tags": []
    },
    "keadex-mina": {
      "root": "apps/keadex-mina",
      "sourceRoot": "apps/keadex-mina/src",
      "projectType": "application",
      "targets": {
        "dev": {
          "executor": "nx:run-script",
          "options": {
            "script": "tauri dev"
          }
        },
        "info": {
          "executor": "nx:run-script",
          "options": {
            "script": "tauri info"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/keadex-mina/**/*.{ts,tsx,js,jsx}"],
            "eslintConfig": "apps/keadex-mina/.eslintrc.json",
            "ignorePath": "apps/keadex-mina/.eslintignore"
          }
        }
      }
    },
    "keadex-ui-kit": {
      "root": "libs/keadex-ui-kit",
      "sourceRoot": "libs/keadex-ui-kit/src",
      "projectType": "library",
      "tags": [],
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/keadex-ui-kit/**/*.{ts,tsx,js,jsx}"],
            "ignorePath": "libs/keadex-ui-kit/.eslintignore"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/keadex-ui-kit"],
          "options": {
            "jestConfig": "libs/keadex-ui-kit/jest.config.ts",
            "passWithNoTests": true
          }
        }
      }
    },
    "c4-model-ui-kit": {
      "root": "libs/c4-model-ui-kit",
      "sourceRoot": "libs/c4-model-ui-kit/src",
      "projectType": "library",
      "tags": [],
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/c4-model-ui-kit/**/*.{ts,tsx,js,jsx}"],
            "ignorePath": "libs/c4-model-ui-kit/.eslintignore"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/c4-model-ui-kit"],
          "options": {
            "jestConfig": "libs/c4-model-ui-kit/jest.config.ts",
            "passWithNoTests": true
          }
        }
      }
    },
    "keadex-utils": {
      "root": "libs/keadex-utils",
      "sourceRoot": "libs/keadex-utils/src",
      "projectType": "library",
      "tags": [],
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/keadex-utils/**/*.{ts,tsx,js,jsx}"],
            "ignorePath": "libs/keadex-utils/.eslintignore"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/keadex-utils"],
          "options": {
            "jestConfig": "libs/keadex-utils/jest.config.ts",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "$schema": "./node_modules/nx/schemas/workspace-schema.json"
}

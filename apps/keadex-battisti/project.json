{
  "name": "keadex-battisti",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/keadex-battisti",
  "projectType": "application",
  "targets": {
    "clear": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn del apps/keadex-battisti/public/_pagefind"
      }
    },
    "build-code": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/keadex-battisti",
        "webpack": true
      },
      "configurations": {
        "development": {
          "outputPath": "apps/keadex-battisti"
        },
        "production": {}
      },
      "dependsOn": ["lint"]
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn pagefind --site dist/apps/keadex-battisti/.next/server/app/en/docs --output-path dist/apps/keadex-battisti/public/_pagefind"
      },
      "dependsOn": ["clear", "build-code"]
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "keadex-battisti:build-code",
        "dev": true,
        "webpack": true
      },
      "configurations": {
        "development": {
          "buildTarget": "keadex-battisti:build-code:development",
          "dev": true,
          "experimentalHttps": true
        },
        "production": {
          "buildTarget": "keadex-battisti:build-code:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "keadex-battisti:build-code:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/keadex-battisti/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/keadex-battisti/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
